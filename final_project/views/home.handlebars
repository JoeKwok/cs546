    <div id="map"></div>
    <script>
      var map;
      function initMap() {
        var myLatLng = {lat: 40.7468708, lng: -74.0280354};
        map = new google.maps.Map(document.getElementById('map'), {
          center: myLatLng,
          zoom: 15
        });

        var marker = new google.maps.Marker({
          position: myLatLng,
          map: map,
          title: 'Click to zoom'
        });

        var contentString = "Hello Google Map API!";
        var vedioString = '<video width="320" height="240" src="/uploads/myvideo.MOV" controls></video>';

        var myWindow = new google.maps.InfoWindow({
          content: vedioString
        });
        marker.addListener('click', () => {
          //map.setZoom(8);
          //map.setCenter(marker.getPosition());
          myWindow.open(map, marker);
        });

{{#each videos}}
		

        var marker{{@index}} = new google.maps.Marker({
          position: {lat: {{lat}}, lng: {{lon}} },
          map: map,
          title: 'Click to zoom'
        });

        var vedioString = '<video width="320" height="240" src="/uploads/{{name}}" controls></video>';

        var myWindow = new google.maps.InfoWindow({
          content: vedioString
        });
        marker{{@index}}.addListener('click', () => {
          myWindow.open(map, marker{{@index}});
        });
{{/each}}


      }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBXIMiQ_d6uSkP1OV2uzmYf2VACuWEZnFg&callback=initMap"
    async defer></script>
